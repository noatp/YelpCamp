<%= layout("layouts/boilerplate") %>
<h1>All Campgrounds</h1>
<dir>
    <a href="/campgrounds/new">Add Campground</a>
</dir>
<ul>
    <% for (let campground of campgrounds){ %>
    <li>
        <a href="/campgrounds/<%= campground._id %> "><%= campground.title %> </a></a>
    </li>
    <% } %>
</ul> 
